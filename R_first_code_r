#write thte comment here

primates <- c(3, 5, 9, 15, 40) 

# new set
crabs <- c(100,70,30,5,30)

#first plot in R
plot(primates, crabs) 

#put a color in the plot with col
plot(primates, crabs, col="red") 

#change point character with pch
plot(primates, crabs, col="red", pch=19) 

#character exagration (incrise the side of the point)
plot(primates, crabs, col="red", pch=19, cex=2 ) 

#put the title to the graph
plot(primates, crabs, col="red", pch=19, cex=2, main= "MY FIRST ECOLOGICAL GRAPH") 

#create a dataframe
ecoset<-data.frame (primates, crabs)
ecoset 

#ecological numbers
#mean number of individuals per site

#summary of the data frame
summary(ecoset)
